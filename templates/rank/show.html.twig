{% extends 'base.html.twig' %}

{% block title %}Rank{% endblock %}

{% block body %}
    <h1>Rank</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ rank.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ rank.name }}</td>
            </tr>
            <tr>
                <th>Value</th>
                <td>{{ rank.value ? rank.value|join(', ') : '' }}</td>
            </tr>
            <tr>
                <th>Picture</th>
                <td>{{ rank.picture }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app.rank.index') }}">back to list</a>

    <a href="{{ path('app.rank.edit', {'id': rank.id}) }}">edit</a>

    {{ include('rank/_delete_form.html.twig') }}
{% endblock %}
