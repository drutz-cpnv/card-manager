{% extends 'base.html.twig' %}

{% block title %}PrintRequest{% endblock %}

{% block body %}
    <h1>PrintRequest</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ print_request.id }}</td>
            </tr>
            <tr>
                <th>Created_at</th>
                <td>{{ print_request.createdAt ? print_request.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Printed_at</th>
                <td>{{ print_request.printedAt ? print_request.printedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app.print.index') }}">back to list</a>

    <a href="{{ path('app.print.edit', {'id': print_request.id}) }}">edit</a>

    {{ include('print/_delete_form.html.twig') }}
{% endblock %}
